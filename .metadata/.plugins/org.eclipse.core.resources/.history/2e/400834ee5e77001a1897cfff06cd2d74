import java.util.Arrays;
import java.util.List;

public class sorted {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
static List<User> users=Arrays.asList(new User("krishan", 29),new User("ravi", 26),new User("hari", 19));
	}
	static class User {

        private String name;
        private int age;

        public User(String name, int age) {
            this.name = name;
            this.age = age;
        }

        public String getName() {
            return name;
        }

        public void setName(String name) {
            this.name = name;
        }

        public int getAge() {
            return age;
        }

        public void setAge(int age) {
            this.age = age;
        }

        @Override
        public String toString() {
            return "User{" +
                    "name='" + name + '\'' +
                    ", age=" + age +
                    '}';
        }
    }

}
